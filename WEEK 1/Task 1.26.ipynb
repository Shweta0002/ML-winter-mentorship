   "source": [
    "list = [3, 5, 1, 6, 7, 9, 8, 0]  \n",
    "print (\"The original list is : \" + str(list)) \n",
    "counter = 1\n",
    "res = [] \n",
    "for i in list: \n",
    " if counter > 4 or not (i % 2 != 0): \n",
    "  res.append(i) \n",
    " else: \n",
    "  counter += 1\n",
    "print (\"The filtered list is : \" + str(res)) \n"
   ]
